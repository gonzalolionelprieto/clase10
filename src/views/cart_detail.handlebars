<!DOCTYPE html>
<html>
<head>
  <title>Cart</title>
</head>
<body>
  <h1>Cart</h1>
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Price</th>
        <th>Code</th>
        <th>Quantity</th>
        <!-- Add more columns here as needed -->
      </tr>
    </thead>
    <tbody>
      {{#each products}}
      <tr>
        <td>{{this.title}}</td>
        <td>{{this.description}}</td>
        <td>${{this.price}}</td>
        <td>{{this.code}}</td>
        <td>{{this.quantity}}</td>
        <!-- Add more columns here as needed -->
      </tr>
      {{/each}}
    </tbody>
  </table>
</body>
</html>
